{
  "name": "environment",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "browserslist": [
    "last 4 version",
    "ie >= 11"
  ],
  "scripts": {
    "prod": "webpack --env.mode production",
    "prod-with-server": "node --experimental-modules mockServer/index.mjs & webpack --env.mode production",
